	urldate = {2014-09-09},
	journal = {Cancer Science},
	author = {Ichinose, Junji and Watanabe, Kousuke and Sano, Atsushi and Nagase, Takahide and Nakajima, Jun and Fukayama, Masashi and Yatomi, Yutaka and Ohishi, Nobuya and Takai, Daiya},
