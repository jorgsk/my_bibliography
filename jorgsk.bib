	file = {PubMed Central Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/V85XTPWU/Barrick et al. - 1994 - Quantitative analysis of ribosome binding sites in.pdf:application/pdf}
}

@article{doma_rna_2007,
	title = {{RNA} Quality Control in Eukaryotes},
	volume = {131},
	issn = {0092-8674},
	url = {http://www.sciencedirect.com/science/article/pii/S0092867407013955},
	doi = {16/j.cell.2007.10.041},
	abstract = {Eukaryotic cells contain numerous {RNA} quality-control systems that are important for shaping the transcriptome of eukaryotic cells. These systems not only prevent accumulation of nonfunctional {RNAs} but also regulate normal {mRNAs}, repress viral and parasitic {RNAs}, and potentially contribute to the evolution of new {RNAs} and hence proteins. These quality-control circuits can be viewed as a series of kinetic competitions between steps in normal {RNA} biogenesis or function and {RNA} degradation pathways. These {RNA} quality-control circuits depend on specific adaptor proteins that target aberrant {RNAs} for degradation as well as the coupling of individual steps in {mRNA} biogenesis and function.},
	number = {4},
	urldate = {2011-08-09},
	journal = {Cell},
	author = {Doma, Meenakshi K. and Parker, Roy},
	month = nov,
	year = {2007},
	pages = {660--668},
	annote = {Examples of nuclear retention of {aberrantmRNAs} include the retention of {mRNAs} with defectsin splicing or polyadenylation in both yeast and mam-malian cells (Custodio et al., 1999; Hilleren et al., 2001;Jensen et al., 2001). Interestingly, these aberrant {RNAsare} retained in the vicinity of the gene (== {CHROMATIN}?)
~Similarly, when mammalian intron-containing {mRNAs} are lacking their introns, the resultingaberrant {mRNAs} are more rapidly degraded and accumu-late in the nucleus when degradation is inhibited (Conradet al., 2006)},
	file = {ScienceDirect Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/S34VGBJE/Doma and Parker - 2007 - RNA Quality Control in Eukaryotes.pdf:application/pdf;ScienceDirect Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/MAXDWUE5/S0092867407013955.html:text/html}
}

@article{qian_simple_1997,
	title = {A simple theory of motor protein kinetics and energetics},
	volume = {67},
	issn = {0301-4622},
	url = {http://www.sciencedirect.com/science/article/pii/S0301462297000513},
	doi = {10.1016/S0301-4622(97)00051-3},
	abstract = {A simple stochastic theory for kinetics and energetics of the movement of single motor proteins is presented. The model combines the biochemical cycle of nucleotide hydrosis with the motor protein translocation. Based on the theory of Markov processes, the model provides the force-velocity relationship, the isometric force, and the stochastic stepping of the motor protein along its one-dimensional track. The theoretical model provides a conceptual framework for realistic studies of motor proteins. Relationship between the present theory and other existing models is discussed.},
	number = {1{\textendash}3},
