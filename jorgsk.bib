	pages = {1419--1431},
	file = {PubMed Central Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/MSZQFTVJ/Margeat et al. - 2006 - Direct Observation of Abortive Initiation and Prom.pdf:application/pdf}
}

