	pages = {4953--4957},
	file = {PubMed Central Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/HU7UNSDM/Chen et al. - 1994 - Determination of the optimal aligned spacing betwe.pdf:application/pdf}
}

