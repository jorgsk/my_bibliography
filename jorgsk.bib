	pages = {1617--1631},
	file = {Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/CR45C7VV/Schlackow et al. - 2013 - Genome-wide analysis of poly(A) site selection in .pdf:application/pdf;Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/3PWM2DNZ/1617.html:text/html}
