	doi = {10.1074/jbc.M109.053983},
	abstract = {Bacterial transcription elongation factors {GreA} and {GreB} stimulate the intrinsic {RNase} activity of {RNA} polymerase ({RNAP}), thus helping the enzyme to read through pausing and arresting sites on {DNA}. Gre factors also accelerate {RNAP} transition from initiation to elongation. Here, we characterized the molecular mechanism by which Gre factors facilitate transcription at two promoters, P and P, that require {GreA} for optimal activity. Using transcription assays, {KMnO} footprinting, and Fe-induced hydroxyl radical mapping, we show that during transcription initiation at P and P in the absence of Gre factors, {RNAP} falls into a condition of promoter-proximal transcriptional arrest that prevents production of full-length transcripts both and . Arrest occurs when {RNAP} synthesizes 9{\textendash}14-nucleotide-long transcripts and backtracks by 5{\textendash}7 (P) or 2{\textendash}4 (P) nucleotides. Initiation factor $\sigma$ contributes to the formation of arrested complexes at both promoters. The signal for promoter-proximal arrest at P is bipartite and requires two elements: the extended -10 promoter element and the initial transcribed region from positions +2 to +6. {GreA} and {GreB} prevent arrest at P and P by inducing cleavage of the 3'-proximal backtracked portion of {RNA} at the onset of arrested complex formation and stimulate productive transcription by allowing {RNAP} to elongate the 5'-proximal transcript cleavage products in the presence of substrates. We propose that promoter-proximal arrest is a common feature of many bacterial promoters and may represent an important physiological target of regulation by transcript cleavage factors.},
	number = {51},
	urldate = {2010-04-12},
	journal = {Journal of Biological Chemistry},
	author = {Stepanova, Ekaterina and Wang, Minshi and Severinov, Konstantin and Borukhov, Sergei},
	month = dec,
	year = {2009},
	pages = {35702--35713},
	file = {Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/B7HT8SU2/Stepanova et al. - 2009 - Early Transcriptional Arrest at Escherichia coli r.pdf:application/pdf;Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/CFZRPSP6/Stepanova et al. - 2009 - Early Transcriptional Arrest at Escherichia coli r.pdf:application/pdf;Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/373WVBC8/35702.html:text/html;Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/MB9ZB26N/35702.html:text/html}
}

