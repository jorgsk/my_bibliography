	issn = {0022-2836},
	url = {http://www.sciencedirect.com/science/article/pii/S0022283612006286},
	doi = {10.1016/j.jmb.2012.07.027},
	abstract = {Rho termination factor is an essential hexameric helicase responsible for terminating 20{\textendash}50\% of all {mRNA} synthesis in Escherichia coli. We used single-molecule force spectroscopy to investigate Rho{\textendash}{RNA} binding interactions at the Rho utilization site of the $\lambda${tR}1 terminator. Our results are consistent with Rho complexes adopting two states: one that binds 57 {\textpm} 2 nt of {RNA} across all six of the Rho primary binding sites, and another that binds 85 {\textpm} 2 nt at the six primary sites plus a single secondary site situated at the center of the hexamer. The single-molecule data serve to establish that Rho translocates 5' {\textrightarrow} 3' toward {RNA} polymerase ({RNAP}) by a tethered-tracking mechanism, looping out the intervening {RNA} between the Rho utilization site and {RNAP}. These findings lead to a general model for Rho binding and translocation and establish a novel experimental approach that should facilitate additional single-molecule studies of {RNA}-binding proteins.},
	urldate = {2012-09-03},
	journal = {Journal of Molecular Biology},
	author = {Koslover, Daniel J. and Fazal, Furqan M. and Mooney, Rachel A. and Landick, Robert and Block, Steven M.},
	keywords = {force spectroscopy, optical trap, {RNA} polymerase, single-molecule biophysics, transcription},
	file = {ScienceDirect Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/IU3FT6RF/Koslover et al. - Binding and Translocation of Termination Factor Rh.pdf:application/pdf;ScienceDirect Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/ETT6TA98/S0022283612006286.html:text/html}
}

