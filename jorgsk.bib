	file = {ScienceDirect Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/2ESB2NTN/Raffaelle et al. - 2005 - Holoenzyme Switching and Stochastic Release of Sig.pdf:application/pdf;ScienceDirect Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/RTD2MCAZ/science.html:text/html}
}

@article{guhaniyogi_regulation_2001,
