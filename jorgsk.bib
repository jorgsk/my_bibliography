@article{goldman_nanornas_2011,
	title = {{NanoRNAs} Prime Transcription Initiation In Vivo},
	volume = {42},
	issn = {1097-2765},
	url = {http://www.sciencedirect.com/science/article/pii/S1097276511004199},
	doi = {10.1016/j.molcel.2011.06.005},
	abstract = {Summary
It is often presumed that, in~vivo, the initiation of {RNA} synthesis by {DNA}-dependent {RNA} polymerases occurs using {NTPs} alone. Here, using the model Gram-negative bacterium Pseudomonas aeruginosa, we demonstrate that depletion of the small-{RNA}-specific exonuclease, Oligoribonuclease, causes the accumulation of oligoribonucleotides 2 to \~{}4 nt in~length, {\textquotedblleft}{nanoRNAs},{\textquotedblright} which serve as primers for transcription initiation at a significant fraction of promoters. Widespread use of {nanoRNAs} to prime transcription initiation is coupled with global alterations in gene expression. Our results, obtained under conditions in which the concentration of {nanoRNAs} is artificially elevated, establish that small {RNAs} can be used to initiate transcription in~vivo, challenging the idea that all cellular transcription occurs using only {NTPs}. Our findings further suggest that {nanoRNAs} could represent a distinct class of functional small {RNAs} that can affect gene expression through direct incorporation into a target {RNA} transcript rather than through a traditional antisense-based mechanism.},
	number = {6},
	urldate = {2012-02-03},
	journal = {Molecular Cell},
	author = {Goldman, Seth R. and Sharp, Josh S. and Vvedenskaya, Irina O. and Livny, Jonathan and Dove, Simon L. and Nickels, Bryce E.},
	month = jun,
	year = {2011},
