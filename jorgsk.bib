	journal = {Bioinformatics},
	author = {Kalkatawi, Manal and Rangkuti, Farania and Schramm, Michael and Jankovic, Boris R and Kamau, Allan and Chowdhary, Rajesh and Archer, John A. C and Bajic, Vladimir B},
	month = nov,
	year = {2011},
	file = {Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/8B2AGGV7/Kalkatawi et al. - 2011 - Dragon PolyA Spotter Predictor of poly(A) Motifs .pdf:application/pdf;Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/NSA5UK83/bioinformatics.btr602.html:text/html}
}

