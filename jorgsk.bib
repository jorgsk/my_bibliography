@article{kapanidis_initial_2006,
	title = {Initial Transcription by {RNA} Polymerase Proceeds Through a {DNA}-Scrunching Mechanism},
	volume = {314},
	issn = {0036-8075, 1095-9203},
	url = {http://www.sciencemag.org/content/314/5802/1144},
	doi = {10.1126/science.1131399},
	abstract = {Using fluorescence resonance energy transfer to monitor distances within single molecules of abortively initiating transcription initiation complexes, we show that initial transcription proceeds through a {\textquotedblleft}scrunching{\textquotedblright} mechanism, in which {RNA} polymerase ({RNAP}) remains fixed on promoter {DNA} and pulls downstream {DNA} into itself and past its active center. We show further that putative alternative mechanisms for {RNAP} active-center translocation in initial transcription, involving {\textquotedblleft}transient excursions{\textquotedblright} of {RNAP} relative to {DNA} or {\textquotedblleft}inchworming{\textquotedblright} of {RNAP} relative to {DNA}, do not occur. The results support a model in which a stressed intermediate, with {DNA}-unwinding stress and {DNA}-compaction stress, is formed during initial transcription, and in which accumulated stress is used to drive breakage of interactions between {RNAP} and promoter {DNA} and between {RNAP} and initiation factors during promoter escape.},
	language = {en},
	number = {5802},
	urldate = {2012-07-26},
	journal = {Science},
	author = {Kapanidis, Achillefs N. and Margeat, Emmanuel and Ho, Sam On and Kortkhonjia, Ekaterine and Weiss, Shimon and Ebright, Richard H.},
	month = nov,
	year = {2006},
	pages = {1144--1147},
	file = {kapanidis.pdf:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/8S8HTITK/kapanidis.pdf:application/pdf;Kapanidis.SOM.pdf:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/IARRE78C/Kapanidis.SOM.pdf:application/pdf;Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/DNU87DH3/1144.html:text/html}
