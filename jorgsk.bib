	abstract = {Summary
Transcriptional pausing by {RNA} polymerase ({RNAP}) plays an important role in the regulation of gene expression. Defined, sequence-specific pause sites have been identified biochemically. Single-molecule studies have also shown that bacterial {RNAP} pauses frequently during transcriptional elongation, but the relationship of these {\textquotedblleft}ubiquitous{\textquotedblright} pauses to the underlying {DNA} sequence has been uncertain. We employed an ultrastable optical-trapping assay to follow the motion of individual molecules of {RNAP} transcribing templates engineered with repeated sequences carrying imbedded, sequence-specific pause sites of known regulatory function. Both the known and ubiquitous pauses appeared at reproducible locations, identified with base-pair accuracy. Ubiquitous pauses were associated with {DNA} sequences that show similarities to regulatory pause sequences. Data obtained for the lifetimes and efficiencies of pauses support a model where the transition to pausing branches off of the normal elongation pathway and is mediated by a common elemental state, which corresponds to the ubiquitous pause.},
	number = {6},
	urldate = {2012-05-16},
	journal = {Cell},
	author = {Herbert, Kristina M. and La Porta, Arthur and Wong, Becky J. and Mooney, Rachel A. and Neuman, Keir C. and Landick, Robert and Block, Steven M.},
	month = jun,
	year = {2006},
	pages = {1083--1094},
	file = {ScienceDirect Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/RGTCHK3A/Herbert et al. - 2006 - Sequence-Resolved Detection of Pausing by Single R.pdf:application/pdf;ScienceDirect Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/KNGHDTAX/Herbert et al. - 2006 - Sequence-Resolved Detection of Pausing by Single R.html:text/html}
}

@article{wang_modeling_2014,
	title = {Modeling {DNA} Thermodynamics under Torsional Stress},
	volume = {106},
	issn = {0006-3495},
	url = {http://www.sciencedirect.com/science/article/pii/S0006349514000927},
	doi = {10.1016/j.bpj.2014.01.022},
	abstract = {Negatively twisted {DNA} is essential to many biological functions. Due to torsional stress, duplex {DNA} can have local, sequence-dependent structural defects. In this work, a thermodynamic model of {DNA} was built to qualitatively predict the local sequence-dependent mechanical instabilities under torsional stress. The results were compared to both simulation of a coarse-grained model and experiment results. By using the Kirkwood superposition approximation, we built an analytical model to represent the free energy difference $\Delta$W of a hydrogen-bonded basepair between the B-form helical state and the basepair opened (or locally melted) state, within a given sequence under torsional stress. We showed that $\Delta$W can be well approximated by two-body interactions with its nearest-sequence-neighbor basepairs plus a free energy correction due to long-range correlations. This model is capable of rapidly predicting the position and thermodynamics of local defects in a given sequence. The result qualitatively matches with an in~vitro experiment for a long {DNA} sequence (\&gt;4000 basepairs). The 12 parameters used in this model can be further quantitatively refined when more experimental data are available.},
	number = {5},
	urldate = {2014-09-07},
	journal = {Biophysical Journal},
	author = {Wang, Qian and Pettitt, B. Montgomery},
	month = mar,
	year = {2014},
	pages = {1182--1193},
	file = {ScienceDirect Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/AG83DC9U/Wang and Pettitt - 2014 - Modeling DNA Thermodynamics under Torsional Stress.pdf:application/pdf;ScienceDirect Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/HEVHFJUR/S0006349514000927.html:text/html}
}

