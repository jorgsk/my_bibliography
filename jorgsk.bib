	file = {Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/DD375FA6/Reis et al. - 2004 - Solving the Riddle of Codon Usage Preferences A T.pdf:application/pdf;Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/2D242ESN/5036.html:text/html}
}

