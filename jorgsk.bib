	journal = {Biochemical and Biophysical Research Communications},
	author = {Park, Young Seoub and Seo, Sang Woo and Hwang, Seungha and Chu, Hun Su and Ahn, Jin-Ho and Kim, Tae-Wan and Kim, Dong-Myung and Jung, Gyoo Yeol},
	month = apr,
	year = {2007},
	keywords = {5'-{UTR}, {AU}-rich sequence, Expressional {PCR}, {SD} sequence, Tunable expression},
	pages = {136--141},
	file = {ScienceDirect Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/XS74BIMK/Park et al. - 2007 - Design of 5'-untranslated region variants for tuna.pdf:application/pdf;ScienceDirect Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/G8N58U4J/science.html:text/html}
