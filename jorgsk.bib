}

@article{min_nonequilibrium_2005,
	title = {Nonequilibrium Steady State of a Nanometric Biochemical System:  Determining the Thermodynamic Driving Force from Single Enzyme Turnover Time Traces},
	volume = {5},
	issn = {1530-6984},
	shorttitle = {Nonequilibrium Steady State of a Nanometric Biochemical System},
