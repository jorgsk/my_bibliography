	month = dec,
	year = {2013},
	pmid = {24152550},
	keywords = {fission yeast, genome-wide, Polyadenylation, {RNA}-seq, transcription termination},
	pages = {1617--1631},
	file = {Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/3K9PJTFF/Schlackow et al. - 2013 - Genome-wide analysis of poly(A) site selection in .pdf:application/pdf;Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/V63XTM2X/1617.html:text/html}
