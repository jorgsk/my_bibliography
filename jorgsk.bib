	keywords = {biochemistry, Biophysics, Chemical biology, structural biology},
	pages = {431--434},
	file = {Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/MDM48DUH/Howan et al. - 2012 - Initiation of transcription-coupled repair charact.pdf:application/pdf;Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/SDJN8TCI/nature11430.html:text/html}
