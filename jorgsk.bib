	file = {Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/8MV228PI/Chan and Gross - 2001 - The Anti-initial Transcribed Sequence, a Portable .pdf:application/pdf;Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/FG6GC7W8/38201.html:text/html}
}

@article{shavkunov_gains_2009,
	title = {Gains and unexpected lessons from genome-scale promoter mapping},
