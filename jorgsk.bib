@article{spanjaard_expression_1989,
	title = {Expression of the rat interferon-$\alpha$1 gene in \textit{Escherichia coli} controlled by the secondary structure of the translation-initiation region},
	volume = {80},
	issn = {0378-1119},
	url = {http://www.sciencedirect.com/science/article/pii/0378111989902989},
	doi = {10.1016/0378-1119(89)90298-9},
	abstract = {A synthetic ribosome-binding site ({RBS}) containing a 7-nucleotide-long Shine-Dalgarno ({SD}) sequence was placed ahead of the rat interferon ({IFN})-$\alpha$1 coding region. The translational efficiency of this construct was extremely low. Structural probing of transcripts with {RNases} T1 and U2 combined with computer predictions revealed the presence of a stable hairpin in which the {SD} region was base-paired to codons 3, 4 and 5 of the {IFN} {mRNA}. Each mutation in this stem changing an A-U to an A {\textperiodcentered} C or a G-C a G {\textperiodcentered} U pair increased translational efficiency about fourfold and this effect could be reversed by a compensating stabilizing substitution in the other strand of the stem. We conclude that the strength of an {RBS} is to a major degree determined by its involvement in secondary structure. We also show that the negative effect of secondary structure on the efficiency of an {RBS} can be overcome by allowing upstream translation to terminate within the base-paired region. In our clones, termination-dependent restarts occur at a frequency comparable to that taking place in constructs containing destabilized hairpins.},
	number = {2},
	urldate = {2012-05-31},
	journal = {Gene},
	author = {Spanjaard, Remco A. and van Dijk, Marc C.M. and Turion, Albert J. and van Duin, Jan},
	month = aug,
	year = {1989},
	keywords = {efficiency of translation, hairpin structure, ribosome-binding site, {RNA} folding, Shine-Dalgarno sequence complementarity, Site-specific mutagenesis, translational restart},
	pages = {345--351},
	file = {ScienceDirect Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/T53WHU7Z/Spanjaard et al. - 1989 - Expression of the rat interferon-$\alpha$1 gene in Escher.pdf:application/pdf;ScienceDirect Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/T23BEGG2/Spanjaard et al. - 1989 - Expression of the rat interferon-$\alpha$1 gene in Escher.html:text/html}
}

