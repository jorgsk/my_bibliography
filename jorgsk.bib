	author = {Isambert, Herv{\'e} and Siggia, Eric D.},
	month = jun,
	year = {2000},
	pmid = {10823910},
	pmcid = {PMC18642},
	pages = {6515--6520},
	file = {PubMed Central Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/SICICWHS/Isambert and Siggia - 2000 - Modeling RNA folding paths with pseudoknots Appli.pdf:application/pdf}
}

