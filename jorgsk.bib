@article{noy_length_2012,
	title = {Length Scale Dependence of {DNA} Mechanical Properties},
	volume = {109},
	url = {http://link.aps.org/doi/10.1103/PhysRevLett.109.228101},
	doi = {10.1103/PhysRevLett.109.228101},
	abstract = {Although mechanical properties of {DNA} are well characterized at the kilobase-pair range, a number of recent experiments have suggested that {DNA} is more flexible at shorter length scales, which correspond to the regime that is crucial for cellular processes such as {DNA} packaging and gene regulation. Here, we perform a systematic study of the effective elastic properties of {DNA} at different length scales by probing the conformation and fluctuations of {DNA} from the single base-pair level up to four helical turns, using trajectories from atomistic simulation. We find evidence that supports cooperative softening of the stretch modulus and identify the essential modes that give rise to this effect. The bend correlation exhibits modulations that reflect the helical periodicity, while it yields a reasonable value for the effective persistence length, and the twist modulus undergoes a smooth crossover{\textemdash}from a relatively smaller value at the single base-pair level to the bulk value{\textemdash}over half a {DNA} turn.},
	number = {22},
	urldate = {2014-08-25},
	journal = {Physical Review Letters},
	author = {Noy, Agnes and Golestanian, Ramin},
	month = nov,
	year = {2012},
	pages = {228101},
	file = {APS Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/EP44FIPH/PhysRevLett.109.html:text/html;Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/WS7NQXXN/Noy and Golestanian - 2012 - Length Scale Dependence of DNA Mechanical Properti.pdf:application/pdf}
}

