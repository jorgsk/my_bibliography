@article{strobel_regulation_2014,
	title = {Regulation of promoter-proximal transcription elongation: enhanced {DNA} scrunching drives $\lambda$Q antiterminator-dependent escape from a $\sigma$70-dependent pause},
	volume = {42},
	issn = {0305-1048, 1362-4962},
