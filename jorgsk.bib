	file = {Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/CKE8X6SH/Forde et al. - 2002 - Using mechanical force to probe the mechanism of p.pdf:application/pdf;Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/HQ3CR2RC/11682.html:text/html}
}

@article{zhang_mrna_2006,
	title = {{mRNA} secondary structure at start {AUG} codon is a key limiting factor for human protein expression in \textit{Escherichia coli}},
