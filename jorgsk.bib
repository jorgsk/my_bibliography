	file = {Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/PVF62TNJ/Mangone et al. - 2010 - The Landscape of C. Elegans 3'UTRs.pdf:application/pdf;Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/NKVAT7UM/432.html:text/html;Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/R6523V8G/432.html:text/html}
