	file = {Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/H7QUCZ8E/Weindl et al. - 2007 - Modeling DNA-binding of Escherichia coli $\sigma$70 exhib.pdf:application/pdf;Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/7ZSHRB99/7003.html:text/html}
}

