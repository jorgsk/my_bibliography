	file = {Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/8NW36XVR/Hernandez et al. - 1996 - Conserved Region 3 of Escherichia Coli $\Sigma$70 Is Impl.pdf:application/pdf;Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/2IMZM53U/18775.html:text/html}
}

@article{komissarova_functional_1998,
	title = {Functional topography of nascent {RNA} in elongation intermediates of {RNA} polymerase},
	volume = {95},
	issn = {0027-8424, 1091-6490},
	url = {http://www.pnas.org/content/95/25/14699},
	abstract = {To determine the dynamics of transcript extrusion from Escherichia coli {RNA} polymerase ({RNAP}), we used degradation of the {RNA} by {RNases} T1 and A in a series of consecutive elongation complexes ({ECs}). In intact {ECs}, even extremely high doses of the {RNases} were unable to cut the {RNA} closer than 14{\textendash}16 nt from the 3' end. Our results prove that all of the cuts detected within the 14-nt zone are derived from the {EC} that is denatured during inactivation of the {RNases}. The protected zone monotonously translocates along the {RNA} after addition of new nucleotides to the transcript. The upstream region of the {RNA} heading toward the 5' end is cleaved and dissociated from the {EC}, with no effect on the stability and activity of the {EC}. Most of the current data suggest that an 8- to 10-nt {RNA}.{DNA} hybrid is formed in the {EC}. Here, we show that an 8- to 10-nt {RNA} obtained by truncating the {RNase}-generated products further with either {GreB} or pyrophosphate is sufficient for the high stability and activity of the {EC}. This result suggests that the transcript{\textendash}{RNAP} interaction that is required for holding the {EC} together can be limited to the {RNA} region involved in the 8- to 10-nt {RNA}.{DNA} hybrid.},
	language = {en},
	number = {25},
	urldate = {2013-01-13},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Komissarova, Natalia and Kashlev, Mikhail},
	month = dec,
	year = {1998},
	pages = {14699--14704},
	file = {Full Text PDF:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/2DIPFTS2/Komissarova and Kashlev - 1998 - Functional topography of nascent RNA in elongation.pdf:application/pdf;Snapshot:/home/jorgsk/.mozilla/firefox/9pejuhur.default/zotero/storage/HCTP8ENG/14699.html:text/html}
}

